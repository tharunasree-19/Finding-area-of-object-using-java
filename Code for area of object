public class Practice{
    public static void main(String args[]){
        int length = 15;
        int width = 36;
        int area = length * width;
        System.out.println("The length of the object : " + length);
        System.out.println("The width of the object : " + width);
        System.out.println("The area of the object : " + area);
        
    }
}
